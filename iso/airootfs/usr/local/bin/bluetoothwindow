# Blue Tooth Devices window launcher for YOUNIX desktop environment

SCRIPT_NAME=$(basename "$0")

flock -n $LOCK_DIR/$SCRIPT_NAME.lock terminal --columns=120 --lines=30 --title="$FLOATING_PREFIX Blue Tooth Devices" bluetui
