# Calendar Window launcher for YOUNIX desktop environment

SCRIPT_NAME=$(basename "$0")

flock -n $LOCK_DIR/$SCRIPT_NAME.lock terminal --columns=150 --lines=40 --title="$FLOATING_PREFIX Calendar" calcurse
